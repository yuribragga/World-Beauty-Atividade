{
  "compilerOptions": {
    "target": "es2016",                                  
    "module": "commonjs",                                
    "esModuleInterop": true,                                                    
    "forceConsistentCasingInFileNames": true,            
    "strict": false,                                      
    "outDir": "out/",
    "moduleResolution": "node",
    "noImplicitAny": false                                
  },
  "include": [
    "src/**/*"
  ]
}
